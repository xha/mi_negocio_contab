    <script type="text/javascript">
        $(document).ready(function () {
            if ($("#menu_actual").val() != "") {
                var arreglo = $("#menu_actual")[0].value.split("-");
                var i;
                var padres = arreglo[0].split("_");
                var hijo = arreglo[1].split("_");
                for (i = 1; i < padres.length; i++) {
                    if (parseInt(padres[i]) > 0) {
                        $("#menu_" + padres[i])[0].click();
                    }
                }

                pagina_central(hijo[0], hijo[1], hijo[2]);
                $("#menu_actual")[0].value = "";
            }

            $.getJSON(ruta_actual + "/Menu/GridMenu", function (data) {
                $("#buscador_principal").typeahead({
                    source: data,
                    autoSelect: false,
                    fitToElement: false,
                    displayText: function (item) { return item.Name; },
                    updater: function (item) {
                        loading_show();
                        var cadena = item.parentId + "_" + item.parentId2 + "_" + item.parentId3 + "-" + item.action + "_" + item.controller + "_" + item.Id;
                        var url = ruta_actual + "/Menu/Index?id=" + item.parentId + "&acc=" + cadena;
                        //console.log(url);
                        window.location.href = url;
                    }
                });
            });            

            if (!$.datepicker.initialized) {
                $(document).mousedown($.datepicker._checkExternalClick)
                    .find(document.body).append($.datepicker.dpDiv);
                $.datepicker.initialized = true;
            }

            $.datepicker.regional['es'] = {
                closeText: 'Cerrar',
                prevText: '< Ant',
                nextText: 'Sig >',
                currentText: 'Hoy',
                monthNames: ['Enero', 'Febrero', 'Marzo', 'Abril', 'Mayo', 'Junio', 'Julio', 'Agosto', 'Septiembre', 'Octubre', 'Noviembre', 'Diciembre'],
                monthNamesShort: ['Ene', 'Feb', 'Mar', 'Abr', 'May', 'Jun', 'Jul', 'Ago', 'Sep', 'Oct', 'Nov', 'Dic'],
                dayNames: ['Domingo', 'Lunes', 'Martes', 'Miércoles', 'Jueves', 'Viernes', 'Sábado'],
                dayNamesShort: ['Dom', 'Lun', 'Mar', 'Mié', 'Juv', 'Vie', 'Sáb'],
                dayNamesMin: ['Do', 'Lu', 'Ma', 'Mi', 'Ju', 'Vi', 'Sá'],
                weekHeader: 'Sm',
                dateFormat: 'dd-mm-yy',
                firstDay: 1,
                isRTL: false,
                showMonthAfterYear: false,
                yearSuffix: ''
            };
        });

        $(".modal").on("hidden.bs.modal", function () {
            $('.modal-dialog').css({
                top: 0,
                left: 0
            });
        });

        $('.modal-dialog').draggable({
            handle: ".modal-header"
        });

        $('.dataTables').DataTable({
            responsive: true
        });

        $('[data-toggle="tooltip"]').tooltip({
            placement: 'top', // or bottom, left, right, and variations
            title: "Top"
        });

        $.datepicker.setDefaults($.datepicker.regional['es']);

        $(".datepickers").datepicker({
            autoclose: true
        });

        $('.telefono').mask('(000) 000-0000', { placeholder: "(000) 000-0000" });

        $('input[type=radio]').iCheck({
            radioClass: 'iradio_square-blue',
        });

        $('input[type=checkbox]').iCheck({
            checkboxClass: 'icheckbox_square-blue',
        });

        function panel_ayuda() {
            if ($('#sidebar-primer')[0].style.width == '50%') {
                $('#sidebar-primer')[0].style.width = '0';
            } else {
                $('#sidebar-primer')[0].style.width = '50%';
            }
        }

        function ayuda_contenido(route) {
            var ruta = ruta_actual + "/" + route;
            var param = "";
            $.ajax({
                type: 'GET',
                url: ruta,
                data: param,
                cache: false,
                beforeSend: AjaxWait,
                success: AjaxSucceeded,
                error: AjaxFailed
            });
            function AjaxWait() {
                $('#sidebar-primer').html("<img class='w-100' src='../Content/images/loading1.gif' />");
            }
            function AjaxSucceeded(result) {
                $("#sidebar-primer").load(ruta);
            }
            function AjaxFailed(result) {
                $('#sidebar-primer').html("Error al Cargar la Solicitud");
            }
        }

        function cambia_icono(valor) {
            if ($("#icono_"+valor).hasClass( "fa-angle-right")) {
                $("#icono_" + valor).removeClass("fa-angle-right");
                $("#icono_" + valor).addClass("fa-angle-down");
            } else {
                $("#icono_" + valor).removeClass("fa-angle-down");
                $("#icono_" + valor).addClass("fa-angle-right");
            }
        }
    </script>
    @RenderSection("scripts", required: false)
</body>
</html>
