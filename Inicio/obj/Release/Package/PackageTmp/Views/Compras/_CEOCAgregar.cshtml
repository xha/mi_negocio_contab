<div class="card shadow">
    <div class="card-header py-3">
        <h6 class="m-0 font-weight-bold">Estado de Orden de Compra No. @ViewBag.Documento</h6>
    </div>
    <div class="card-body">
        <div class="table-responsive">
            <table class="table table-bordered pre-scrollable tabla_modal" id="CEOCAgregarGrid" width="100%" cellspacing="0">
                <thead>
                    <tr>
                        <th>C&oacute;digo</th>
                        <th>Descripci&oacute;n</th>
                        <th>EOC Manual</th>
                        <th>EOC Sistema</th>
                    </tr>
                </thead>
                <tbody>
                    <tr>
                        <td>D</td>
                        <td>Aprobada</td>
                        <td>0</td>
                        <td>0</td>
                    </tr>
                    <tr>
                        <td>E</td>
                        <td>Emitida</td>
                        <td>0</td>
                        <td>0</td>
                    </tr>
                    <tr>
                        <td>L</td>
                        <td>Liquidada</td>
                        <td>0</td>
                        <td>0</td>
                    </tr>
                    <tr>
                        <td>P</td>
                        <td>Entregada Parcial</td>
                        <td>0</td>
                        <td>0</td>
                    </tr>
                    <tr>
                        <td>R</td>
