                        <td>0</td>
                        <td>0</td>
                    </tr>
                    <tr>
                        <td>T</td>
                        <td>Entrega Total</td>
                        <td>0</td>
                        <td>0</td>
                    </tr>
                </tbody>
            </table>
        </div>
        <hr />
        <div class="form-group row">
            <div class="col-sm-12">
                <a href="#" onclick="$('#m_central').modal('hide')" class="btn btn-info">
                    <i class='fa fa-arrow-left'></i>
                    Regresar
                </a>
                <button class="btn btn-success">
                    <i class='fa fa-save'></i>
                    Aceptar
                </button>
            </div>
        </div>
    </div>
</div>
