@model Datos.Models.AlmacenesModel

@{
    ViewBag.Title = "Guía de Remisión";
    Layout = null; //Layout = "~/Views/Shared/_LayoutTab.cshtml";
}

<div class="container-fluid">
    <div class="row">
        <div class="col-sm-12">
            <nav aria-label="breadcrumbs">
                <ol class="breadcrumbs">
                    <li class="breadcrumbs-item">@Html.ActionLink("Almacenes", "Index", "Almacenes")</li>
                    <li class="breadcrumbs-item">Transacciones</li>
                    <li class="breadcrumbs-item"><a href="#" onclick='pagina_central("GGIndex", "Almacenes")'>Gu&iacute;a de Remisi&oacute;n</a></li>
                    <li class="breadcrumbs-item active">Editar</li>
                </ol>
            </nav>
        </div>
        <div class="col-sm-11">
            <h5> <b>Gu&iacute;a de Remisi&oacute;n</b></h5>
            <h6><label style="color:#ffbc00">Editar Registro</label> #<strong>@Model.Codigo</strong></h6>
        </div>
        
        <div class="col-sm-1">
            <a href="#" onclick="AjaxModalGet('Ayuda @ViewBag.Title - Crear / Editar', '/Almacenes/Ayuda', 'GGDatos', 'm_ayudas')" class="icono_ayuda">
                <i class="fa fa-question-circle fa-2x"></i>
            </a>
        </div>
    </div>
    <hr />
    @Html.Partial("GGDatos")
</div>
