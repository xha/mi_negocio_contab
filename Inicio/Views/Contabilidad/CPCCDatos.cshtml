@model Datos.ContabilidadModels.ContabilidadModel

<div class="row">
  <div class="col-sm-2">
    <label>C&oacute;digo de Cuenta</label>
    <input class="form-control alert-warning" ondblclick="AjaxModalGet('Centros de Costo', '/PMaestra/Referencias', '', 'm_middle');" />
  </div>
  <div class="col-sm-4">
    <label>Descripci&oacute;n</label>
    <input class="form-control" readonly />
  </div>
  <div class="col-sm-3">
    <label>Fecha Inicio</label>
    <input class="form-control datepickers" readonly />
  </div>
  <div class="col-sm-3">
    <label>Fecha Final</label>
    <input class="form-control datepickers" readonly />
  </div>
</div>
<hr />
<div class="row">
  <div class="col-sm-12">
    <a href="#" onclick="ocultar_panel('CPCCIndex', 'Contabilidad')" class="btn btn-info">
