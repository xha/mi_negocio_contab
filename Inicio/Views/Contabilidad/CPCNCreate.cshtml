@model Datos.ContabilidadModels.ContabilidadModel

@{
    ViewBag.Title = "Plan de Cuentas Nacional";
    Layout = null; //Layout = "~/Views/Shared/_LayoutTab.cshtml";
}

<div class="container-fluid">
    <div class="row">
        <div class="col-sm-12">
            <nav aria-label="breadcrumbs">
              <ol class="breadcrumbs">
                <li class="breadcrumbs-item">@Html.ActionLink("Contabilidad", "Index", "Contabilidad")</li>
                <li class="breadcrumbs-item">Base de Datos</li>
                <li class="breadcrumbs-item">Plan de Cuentas</li>
                <li class="breadcrumbs-item active">@ViewBag.Title</li>
                <li class="breadcrumbs-item active">Nuevo</li>
              </ol>
            </nav>
        </div>
        <div class="col-sm-11">
            <h5> <b>@ViewBag.Title</b></h5>
            <h6><label style="color:#ffbc00">Nuevo Registro</label></h6>
        </div>
        
        <div class="col-sm-1">
            <a href="#" onclick="AjaxModalGet('Ayuda @ViewBag.Title - Crear / Editar', '/Almacenes/Ayuda', 'CPCNDatos', 'm_ayudas')" class="icono_ayuda">
                <i class="fa fa-question-circle fa-2x"></i>
            </a>
        </div>
    </div>
    <hr />
    @Html.Partial("CPCNDatos")
</div>
